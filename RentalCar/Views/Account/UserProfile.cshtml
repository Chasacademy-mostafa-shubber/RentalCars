@model Profile

<div class="container mt-5">

    <div class="card" id="UserProfile-Card">
        <div class="card-header">
           <h3>Profile</h3>
        </div>
        <div class="card-body">
        <h3>@Model.FullName<br />
            @Model.Address<br />
            @Model.City<br />
            @Model.ZipCode<br />
            @Model.Phone<br />
        </h3>
            <a asp-action="EditProfile"  class="btn btn-primary">Edit profile</a>
        </div>
    </div>


</div>

